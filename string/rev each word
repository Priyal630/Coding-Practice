static String reverseEachWord (String s){
  String result = "";
  String word = "";
  for (int i =0; i <s.length(); i++){
    char c = s.charAt(i);
    if (c!= ''){
      word = c + word;
      } else {
          result = result + word + '';
          word = '';
      }
    }
  result = result + word;
  return result;
}
