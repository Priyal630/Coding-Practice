static String removeDuplicates(String s) {
    String result = "";
    for (int i = 0; i < s.length(); i++) {
        char ch = s.charAt(i);
        boolean found = false;
        for (int j = 0; j < result.length(); j++) {
            if (result.charAt(j) == ch) {
                found = true;
                break;
            }
        }
        if (!found)
            result = result + ch;
    }
    return result;
}
